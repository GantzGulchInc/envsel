  "scripts": [
    {
      "id": "script-emacs",
      "name": "Emacs",
      "ifSet": "",
      "commands": [
        {
          "command": "print",
          "args": [
            "export EDITOR=\"emacs -nw\""
          ]
        }
      ]
    },
    {
      "id": "script-java",
      "name": "Java",
      "ifSet": "JAVA_HOME",
      "commands": [
        {
          "command": "print",
          "args": [
            "export JAVA_HOME=#{JAVA_HOME}",
            "export JRE_HOME=#{JAVA_HOME}",
            "export PATH=#{JAVA_HOME}/bin:${PATH}"
          ]
        }
      ]
    },
    {
      "id": "script-maven",
      "name": "Maven",
      "ifSet": "MAVEN_HOME",
      "commands": [
        {
          "command": "print",
          "args": [
            "export MAVEN_HOME=#{MAVEN_HOME}",
            "export PATH=#{MAVEN_HOME}/bin:${PATH}"
          ]
        }
      ]
    },
    {
      "id": "script-maven-opts",
      "name": "Maven Options",
      "ifSet": "MAVEN_OPTS",
      "commands": [
        {
          "command": "print",
          "args": [
            "export MAVEN_OPTS=#{MAVEN_OPTS}"
          ]
        }
      ]
    },
    {
      "id": "script-ant",
      "name": "Apache ANT",
      "ifSet": "ANT_HOME",
