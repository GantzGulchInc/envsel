				{
					"op": "print",
					"args": [
						"export ECLIPSE_HOME=#{ECLIPSE_HOME}",
						"export PATH=#{ECLIPSE_HOME}:${PATH}"
					]
				}
			]
		},
		{
			"id": "script-nodejs",
			"name": "NodeJS",
			"ifSet": "NODEJS_HOME",
			"instructions": [
				{
					"op": "print",
